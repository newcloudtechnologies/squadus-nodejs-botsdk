{
    "name": "squadus.botsdk.examples",
    "version": "0.0.1",
    "description": "Examples for @squadus/botsdk",
    "scripts": {
        "build": "rm -rf dist && yarn tsc",
        "lint": "eslint .",
        "connect": "npx ts-node src/connect.ts",
        "get-settings": "npx ts-node src/getSettings.ts",
        "create-direct": "npx ts-node src/createDirectRoom.ts",
        "send-message": "npx ts-node src/sendMessage.ts",
        "send-message:thread": "npx ts-node src/sendMessageToThread.ts",
        "send-attachment": "npx ts-node src/sendAttachment.ts",
        "read-thread": "npx ts-node src/readThread.ts",
        "add-remove-channel-user": "npx ts-node src/addRemoveUserToChannel.ts",
        "create-channel:public": "npx ts-node src/createPublicChannel.ts",
        "create-channel:private": "npx ts-node src/createPrivateChannel.ts",
        "change-role": "npx ts-node src/changeUserRole.ts",
        "edit-channel": "npx ts-node src/editChannel.ts",
        "subscribe-messages": "npx ts-node src/subscribeMessages.ts",
        "subscribe-room": "npx ts-node src/subscribeRoom.ts",
        "subscribe-reaction": "npx ts-node src/subscribeReaction.ts"
    },
    "repository": {
